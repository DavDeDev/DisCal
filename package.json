{
  "name": "discal",
  "version": "1.0.0",
  "description": "Discord bot that can read and write from Google Calendar",
  "main": "./build/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "lint": "eslint ./src/ --ext .ts",
